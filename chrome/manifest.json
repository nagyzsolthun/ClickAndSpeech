{
	"manifest_version": 2,
	"name": "text reader",
	"description": "text reader using google's text to speech api",
	"version": "0.1",
	"content_scripts": [{
		"matches": ["<all_urls>"],
		"js": ["content/content.js"]
	}],
	"permissions": ["http://*/","https://*/", "storage"],
	"background" : {"page": "background/background.html"},
	"options_page": "options/html/options.html",
	"browser_action": {
		"default_icon": "icon64.png",
		"default_popup": "popup/popup.html"
	}
}
