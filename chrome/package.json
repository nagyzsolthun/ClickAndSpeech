{
  "name": "click2speech",
  "version": "1.5.8",
  "autohor": "nagyzsolthun",
  "scripts": {
      "build": "npm run clean-build && npm run build-manifest && npm run build-locales && npm run build-img && npm run build-background && npm run build-options && npm run build-popup && npm run build-content && npm run test",
      "test": "node node_modules/webpack/bin/webpack.js --config=webpack.config.tests.js && node node_modules/jasmine/bin/jasmine.js ../build-test/test-bundle.js",
      "clean-build": "rm -rf ../build-extension/ && mkdir ../build-extension",
      "build-manifest": "node manifest.build.js > ../build-extension/manifest.json",
      "build-locales": "cp -TR _locales ../build-extension/_locales",
      "build-img": "mkdir -p ../build-extension/img && cp img/iconOff32.png img/iconOn32.png img/icon64.png ../build-extension/img/",
      "build-background": "node node_modules/webpack/bin/webpack.js --config=webpack.config.background.js",
      "build-options": "ng build --prod",
      "build-popup": "cp -TR popup ../build-extension/popup/",
      "build-content": "cp -TR content ../build-extension/content"
  },
  "dependencies": {
    "@angular/common": "^7.1.1",
    "@angular/compiler": "^7.1.1",
    "@angular/core": "^7.1.1",
    "@angular/forms": "^7.1.1",
    "@angular/http": "^7.1.1",
    "@angular/platform-browser": "^7.1.1",
    "@angular/platform-browser-dynamic": "^7.1.1",
    "@angular/router": "^6.1.0",
    "core-js": "^3.0.1",
    "rxjs": "^6.0.0",
    "zone.js": "~0.9.0"
  },
  "devDependencies": {
    "webpack": "4.30.0",
    "webpack-cli": "3.3.1",
    "find": "0.3.0",
    "jasmine": "3.4.0",
    "ts-loader": "5.4.5",
    "@babel/core": "7.4.4",
    "babel-loader": "8.0.5",
    "@angular-devkit/build-angular": "~0.13.8",
    "@angular/cli": "~7.3.8",
    "@angular/compiler-cli": "^7.1.1",
    "@angular/router": "7.2.14",
    "@types/jasmine": "~3.3.1",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "~11.13.8",
    "codelyzer": "~5.0.1",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~8.1.0",
    "tslint": "~5.16.0",
    "typescript": "~3.4.5",
    "@types/chrome": "0.0.83"
  }
}
